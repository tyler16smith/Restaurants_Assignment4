@{
    ViewBag.Title = "Restaurants List";
}

@* increments the model *@
@model IEnumerable<string>

<div>
    <h1>Provo's Top 5 Restaurants</h1>
    <p>Below are my top five favorite restaurants in the Provo, Utah area</p>
    <ul class="list-group">
        @foreach (string rest in Model)
        {
            //Display the raw html to enable return carriages
            <li class="list-group-item">@Html.Raw(rest)</li>
        }
    </ul>
</div>